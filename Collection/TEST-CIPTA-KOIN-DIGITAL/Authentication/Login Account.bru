meta {
  name: Login Account
  type: http
  seq: 1
}

post {
  url: {{API_URL}}/auth/account
  body: none
  auth: basic
}

headers {
  Origin: {{Origin}}
}

auth:basic {
  username: root@app.com
  password: 123456
}

script:post-response {
  var jsonData = res.getBody();
  bru.setVar("Bearer", jsonData.data.token);
}

settings {
  encodeUrl: true
  timeout: 0
}
